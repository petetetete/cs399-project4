<?xml version='1.0' encoding='utf-8'?>
<widget id="ph289.project4.cs399" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Punch Line</name>
    <description>
        Hit dudes.
    </description>
    <author email="ph289@nau.edu">
        Peter Huettl
    </author>
    <icon src="www/assets/pointer.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
</widget>
